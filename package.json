  "moduleRoots": "src",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.0",
    "cross-env": "^6.0.3",
    "husky": "^3.0.9",
    "nyc": "^14.1.1"
  }
