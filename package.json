    "i18next-xhr-backend": "^3.0.0",
    "jest": "^24.9.0",
    "moment": "^2.24.0",
    "prettier": "^1.18.2",
    "react": "^17.0.1",
    "react-dom": "^16.10.2",
    "react-i18next": "^10.13.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
