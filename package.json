    "redux-thunk": "^2.3.0",
    "typecheck": "^0.1.2",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-ci": "set CI=true && react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
